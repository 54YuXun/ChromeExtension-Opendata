  {
    "name":"iGOD",
    "description":"This extension allows the user download 'CSV' format OpenData to SQL and analyze data.",
    "version":"1.0",
    "manifest_version":2,
    
    "browser_action":{
        "default_icon":"icon/google-plus.png",
        "default_popup":"popup.html"

    },
    "background":{
        "scripts": ["./js/jquery.js","./js/background.js"]
        
      },
    "permissions":[
        "activeTab",
        "tabs",
        "<all_urls>",
        "https://data.gov.tw/*"
        
    ]
    
}